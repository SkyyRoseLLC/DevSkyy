# DevSkyy Staging Environment Variables
# Copy this file to .env.staging.local and customize for your staging environment

# API Configuration
VITE_API_URL=https://staging-api.devskyy.com

# Application
VITE_APP_NAME=DevSkyy
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=staging

# Feature Flags (Staging)
VITE_ENABLE_VOICE=true
VITE_ENABLE_3D=true
VITE_ENABLE_SOCIAL_AUTH=true

# Staging Settings
VITE_ENABLE_DEVTOOLS=false
VITE_LOG_LEVEL=info
