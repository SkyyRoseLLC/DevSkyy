# DevSkyy Enhanced Platform - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Core Configuration
MONGODB_URL=mongodb://localhost:27017/devskyy
NODE_ENV=production
CORS_ORIGINS=http://localhost:3000,https://your-domain.com

# AI Services
OPENAI_API_KEY=your_openai_api_key_here

# WordPress Integration (GOD MODE Level 2)
WORDPRESS_URL=https://your-wordpress-site.com
WORDPRESS_USERNAME=your_username
WORDPRESS_PASSWORD=your_application_password

# WooCommerce Integration
WOOCOMMERCE_CONSUMER_KEY=ck_your_consumer_key
WOOCOMMERCE_CONSUMER_SECRET=cs_your_consumer_secret

# Social Media Automation
INSTAGRAM_ACCESS_TOKEN=your_instagram_token
FACEBOOK_PAGE_ACCESS_TOKEN=your_facebook_token
TIKTOK_ACCESS_TOKEN=your_tiktok_token

# Email & SMS Automation
SENDGRID_API_KEY=SG.your_sendgrid_key
TWILIO_ACCOUNT_SID=ACyour_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token

# Server Access (GOD MODE Level 2)
SSH_HOST=your_server_host
SSH_USERNAME=your_ssh_username
SSH_KEY_PATH=/path/to/private/key
SFTP_ROOT_PATH=/var/www/html

# Advanced Configuration
MONITORING_INTERVAL=30
AUTO_FIX_ENABLED=true
EXECUTIVE_MODE=true
LUXURY_BRAND_MODE=true

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Database
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://user:password@localhost/dbname

# Performance
CACHE_TTL=3600
MAX_WORKERS=4
RATE_LIMIT_PER_MINUTE=100