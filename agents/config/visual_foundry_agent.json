      "execution_mode": "asynchronous",
      "validation_steps": ["quality_check", "brand_consistency", "technical_specs"]
    },
    "TEST": {
      "command": "TEST(job_id)",
      "description": "Validate visual quality, brand consistency, and technical specifications",
      "execution_mode": "synchronous",
      "tools": ["aesthetic_scorer", "clip_similarity", "technical_validator"],
      "coverage_requirement": 90
    },
    "REVIEW": {
      "command": "REVIEW(job_id)",
      "description": "Human review for high-value or complex visual assets",
      "execution_mode": "synchronous",
      "review_criteria": ["aesthetic_quality", "brand_alignment", "technical_excellence"]
    },
    "DEPLOY": {
      "command": "DEPLOY(env)",
      "description": "Deploy visual assets to CDN and asset management system",
      "execution_mode": "batch",
      "cdn": "cloudflare",
      "asset_management": "dam_system"
    }
